    Dbg.enable_log(\"h:/hou_log.txt\")
    Dbg.clear_log()
    Dbg.log_d(\"HIP_FOLDER : \" + hou.getenv(\"GS_HIP_FOLDER\"))
except Exception,e:
    f = open('h:/hou_fatal.txt', 'w')
    f.write(str(e))
    f.close()"	)
}

